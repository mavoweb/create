{
  "name": "grapesjs-mavo",
  "version": "0.1.97",
  "description": "Mavo Create",
  "main": "dist/grapesjs-mavo.min.js",
  "scripts": {
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "build": "npm run v:patch && webpack --env.production && npm run build:css",
    "start": "webpack-dev-server --open --progress --colors & npm run build:css -- -w",
    "build:css": "node-sass src/style/main.scss dist/grapesjs-mavo.min.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "mavo",
    "wysiwyg"
  ],
  "author": "Francesca Cicileo",
  "license": "BSD-3-Clause",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "peerDependencies": {
    "grapesjs": "0.x"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^4.8.3",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "grapesjs-aviary": "^0.1.2",
    "grapesjs-component-countdown": "^0.1.2",
    "grapesjs-navbar": "^0.1.5",
    "grapesjs-plugin-export": "^0.1.5",
    "grapesjs-plugin-filestack": "^0.1.1",
    "juice": "^4.1.1"
  }
}
